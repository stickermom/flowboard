# Copy this file to `.env.local` and fill in the real credentials.
# Never commit the populated `.env.local` file to source control.

# Supabase project public URL (required by the frontend)
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"

# Supabase anon/public API key (safe for the browser but keep outside repo)
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"

# Supabase service role key (server-side only, never expose to browser)
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Direct database connection string for Supabase migrations/CLI usage.
# Use a service account password and rotate it if ever exposed.
SUPABASE_DB_URL="postgresql://USER:PASSWORD@HOST:5432/postgres"
